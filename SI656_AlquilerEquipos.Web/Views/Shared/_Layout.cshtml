<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    <link href="@Url.Content("~/Resources/vendor/sweetalert2/sweetalert2.min.css")" rel="stylesheet" type="text/css" />
    <link href="/Resources/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <link href="/Resources/css/sb-admin-2.min.css" rel="stylesheet">
    @*<link href="/Resources/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">*@
    <link href="@Url.Content("~/Resources/vendor/datatables-orignal/jquery.dataTables.css")" rel="stylesheet" type="text/css" />
    <link href="/Resources/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    @Scripts.Render("~/bundles/modernizr")
</head>
<body id="wrapper">

    <!-- Sidebar -->
    <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion" id="accordionSidebar">

        <!-- Sidebar - Brand -->
        <a class="sidebar-brand d-flex align-items-center justify-content-center" href="#">
            <div class="sidebar-brand-icon">
                <i class="fas fa-snowboarding"></i>
            </div>
            <div class="sidebar-brand-text mx-3">Sports</div>
        </a>

        <!-- Divider -->
        <hr class="sidebar-divider">

        <li class="nav-item">
            <a class="nav-link" href="/Alquiler/Index">
                <i class="fas fa-fw fa-cash-register"></i>
                <span>Alquileres</span>
            </a>
        </li>

        <li class="nav-item">
            <a class="nav-link" href="/DetalleAlquiler/Index">
                <i class="fas fa-fw fa-bars"></i>
                <span>Detalles Alquiler</span>
            </a>
        </li>

        <!-- Nav Item - Charts -->
        <li class="nav-item">
            <a class="nav-link" href="/Cliente/Index">
                <i class="fas fa-fw fa-users"></i>
                <span>Clientes</span>
            </a>
        </li>

        <!-- Nav Item - Tables -->
        <li class="nav-item">
            <a class="nav-link" href="@Url.Action("Index", "Equipo")">
                <i class="fas fa-fw fa-swatchbook"></i>
                <span>Equipos</span>
            </a>
        </li>


    </ul>
    <!-- End of Sidebar -->
    <div class="container-fluid">
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    <script src="/Resources/vendor/jquery/jquery.min.js"></script>
    <script src="/Resources/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="/Resources/vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for all pages-->
    <script src="@Url.Content("~/Resources/vendor/sweetalert2/sweetalert2.min.js")"></script>
    <script src="/Resources/js/sb-admin-2.min.js"></script>
    <script src="@Url.Content("~/Resources/vendor/datatables-original/jquery.dataTables.js")"></script>
    @*<script src="/Resources/vendor/datatables/jquery.dataTables.min.js"></script>*@
    <script src="/Resources/vendor/datatables/dataTables.bootstrap4.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="/Resources/js/demo/datatables-demo.js"></script>
</body>
</html>
